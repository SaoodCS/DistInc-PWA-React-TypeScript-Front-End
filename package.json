{
	"name": "react-typescript-vite-empty-template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"preview": "vite preview",
		"build": "tsc && vite build",
		"ts-errors": "tsc --noEmit --pretty",
		"lint": "eslint . --ignore-path .eslintignore",
		"lint-ts": "concurrently \"npm run ts-errors\" \"npm run lint\" --success all --prefix-colors \"bgBlue.bold,bgMagenta.bold\" --group --names \"TS-ERROR-CHECK,LINT-ERROR-CHECK\"",
		"lint-fix": "eslint . --ignore-path .eslintignore --fix",
		"prettify": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
		"test": "vitest",
		"test-watch": "vitest --watch",
		"view-unused-exports": "npx unimported",
		"loadsw": "node src/global/firebase/sw/script/loadsw.js",
		"loadsw-dev": "(type .env.devdeployed > .env || cat .env.devdeployed > .env) && npm run loadsw",
		"loadsw-prod": "(type .env.proddeployed > .env || cat .env.proddeployed > .env) && npm run loadsw",
		"build-dev": "npm run loadsw-dev && tsc && vite build --mode devdeployed",
		"build-prod": "npm run loadsw-prod && tsc && vite build --mode proddeployed",
		"deploy-dev": "npm run build-dev && firebase deploy --only hosting --project dev-env-39536",
		"deploy-prod": "npm run build-prod && firebase deploy --only hosting --project prod-env-fccfe",
		"start-dev-to-local": "npm run loadsw-dev && vite --mode devlocal --host",
		"start-prod-to-local": "npm run loadsw-prod && vite --mode prodlocal --host",
		"start-dev-to-deployed": "npm run loadsw-dev && vite --mode devdeployed --host",
		"start-prod-to-deployed": "npm run loadsw-prod && vite --mode proddeployed --host"
	},
	"dependencies": {
		"@tanstack/react-query": "^4.33.0",
		"@tanstack/react-query-devtools": "^4.33.0",
		"firebase": "^10.3.0",
		"framer-motion": "^10.16.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-modal-sheet": "^2.0.0",
		"react-router-dom": "^6.15.0",
		"sass": "^1.66.1",
		"styled-components": "^5.3.9",
		"styled-icons": "^10.47.0",
		"vitest": "^0.25.5"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@types/react": "^18.0.37",
		"@types/react-dom": "^18.0.11",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"@typescript-eslint/parser": "^6.4.0",
		"@vitejs/plugin-react": "^4.0.0",
		"@vitest/coverage-c8": "^0.25.5",
		"concurrently": "^8.2.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-security": "^1.7.1",
		"eslint-plugin-unused-imports": "^3.0.0",
		"fs-extra": "^11.1.1",
		"jsdom": "^22.1.0",
		"prettier": "^3.0.1",
		"pwa-asset-generator": "^6.3.1",
		"typescript": "^5.0.2",
		"vite": "^4.3.9",
		"vite-plugin-pwa": "^0.16.4"
	}
}
